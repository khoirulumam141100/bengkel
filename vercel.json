{
  "version": 2,
  "functions": {
    "api/index.php": {
      "runtime": "php"
    },
    "public/index.php": {
      "runtime": "php"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.php"
    },
    {
      "src": "/(.*)",
      "dest": "/public/index.php"
    }
  ]
}
